[
  {
    "context": "vim_mode == normal || EmptyPane || not_editing",
    "bindings": {
      "space space": [
        "task::Spawn",
        { "task_name": "tv:file_finder", "reveal_target": "center" }
      ],
      "space f f": [
        "task::Spawn",
        { "task_name": "tv:find_in_files",  "reveal_target": "center" }
      ],
      "space g g": [
        "task::Spawn",
        { "task_name": "lazygit",  "reveal_target": "center" }
      ],
      "space k 9": [
        "task::Spawn",
        { "task_name": "k9s", "reveal_target": "center" }
      ]
    }
  }
]
